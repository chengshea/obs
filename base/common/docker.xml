<clickhouse>
        <!-- Possible levels [1]:

          - none (turns off logging)
          - fatal
          - critical
          - error
          - warning
          - notice
          - information
          - debug
          - trace
          - test (not for production usage)

            [1]: https://github.com/pocoproject/poco/blob/poco-1.9.4-release/Foundation/include/Poco/Logger.h#L105-L114
        -->
    <logger>
        <level>notice</level>
    </logger>

   <profiles>
    <default>
        <connect_timeout>100</connect_timeout>
        <connect_timeout_with_failover_ms>3000</connect_timeout_with_failover_ms>
        <connect_timeout_with_failover_secure_ms>3000</connect_timeout_with_failover_secure_ms>
        <receive_timeout>1000</receive_timeout>
        <send_timeout>1000</send_timeout>
        <tcp_keep_alive_timeout>600</tcp_keep_alive_timeout>
        <hedged_connection_timeout_ms>300</hedged_connection_timeout_ms>
    </default>
  </profiles>

</clickhouse>
